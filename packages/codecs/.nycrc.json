{
  "check-coverage": false,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "test"
  ],
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".ts",
    ".tsx"
  ],
  "sourceMap": true,
  "instrument": true,
  "all": true
}